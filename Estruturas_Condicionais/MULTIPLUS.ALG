Algoritmo "multiplus"

Var

   x, y : inteiro

Inicio

   escreva("Digite dois numeros inteiros: ")
   leia(x)
   escreva("Digite dois numeros inteiros: ")
   leia(y)


   se (x % y = 0) ou (y % x = 0) entao
      escreva("Sao multiplos")
   senao
        escreval("Nao sao multiplos")

fimse
Fimalgoritmo